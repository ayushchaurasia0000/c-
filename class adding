#include<iostream>

using namespace std;
class ayush 
{
int a,b;
public:
void get(void)
    {
        std::cout <<"enter";
        std::cin>>a>>b;
//a=x;
//b=y;
    }
ayush add(ayush c)
    {
ayush temp;
temp.a = a + c.a;
temp.b= b + c.b;
return temp;
    }

void print (void)
    {
    std::cout <<a<<endl<<b;
    }
};
int main()
{
ayush q,w,e;
q.get();
w.get();
e = q.add(w);
e.print();
return 0;
}
